{
  "name": "sandbox-runner",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/sandbox-runner/src",
  "prefix": "codelab",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": [
        "{workspaceRoot}/dist/{projectRoot}"
      ],
      "options": {
        "project": "libs/sandbox-runner/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/sandbox-runner/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/sandbox-runner/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "libs/sandbox-runner/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "libs/sandbox-runner/**/*.ts",
          "libs/sandbox-runner/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
