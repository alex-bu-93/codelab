{
  "name": "slides",
  "projectType": "library",
  "sourceRoot": "libs/slides/src",
  "prefix": "codelab",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["{workspaceRoot}/dist/{projectRoot}"],
      "options": {
        "project": "libs/slides/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/slides/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/slides/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/slides/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/slides/**/*.ts", "libs/slides/**/*.html"]
      }
    }
  },
  "tags": []
}
