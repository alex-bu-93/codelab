      "prefix": "codelab",
      "schematics": {
        "@nrwl/schematics:component": {
          "style": "scss"
        }
      },
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/codelab/30",
            "index": "apps/angular-thirty-seconds/src/index.html",
