{
  "name": "playground",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/playground/src",
  "prefix": "codelab",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:browser",
      "options": {
        "outputPath": "dist/apps/playground",
        "index": "apps/playground/src/index.html",
        "main": "apps/playground/src/main.ts",
        "polyfills": "apps/playground/src/polyfills.ts",
        "tsConfig": "apps/playground/tsconfig.app.json",
        "assets": [
          "apps/playground/src/favicon.ico",
          "apps/playground/src/assets",
          {
            "glob": "**/*",
            "input": "node_modules/monaco-editor/",
            "output": "./assets/monaco/"
          }
        ],
        "styles": [
          "apps/playground/src/styles.scss"
        ],
        "scripts": []
      },
      "configurations": {
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        },
        "production": {
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "apps/playground/src/environments/environment.ts",
              "with": "apps/playground/src/environments/environment.prod.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "browserTarget": "playground:build:production"
        },
        "development": {
          "browserTarget": "playground:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "browserTarget": "playground:build"
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/playground/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/playground/**/*.ts",
          "apps/playground/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
