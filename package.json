{
  "name": "codelab",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "ng": "node --max_old_space_size=16000 ./node_modules/@angular/cli/bin/ng",
    "//--------------- CODELAB ---------------------": "",
    "start": "ng serve codelab",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "build:bundler": "ts-node -P libs/code-demos/assets/runner/tsconfig.json libs/code-demos/assets/runner/ng-dts/bundler.ts && node libs/code-demos/assets/runner/ng2/ng2-runner.js",
    "build:intro": "ts-node -P libs/intro/generate/tsconfig.json libs/intro/generate/generate-thumbnails.ts",
    "build:intro:verbose": "NODE_DEBUG=cluster,net,http,fs,tls,module,timers ts-node -P libs/intro/generate/tsconfig.json libs/intro/generate/generate-thumbnails.ts",
    "test": "ng test",
    "lint": "ng lint",
    "i18n": "ng xi18n codelab --i18n-format xmb --output-path src/locale",
    "build:ru": "ng build codelab --configuration ru",
    "deploy:next": "npm run build:prod && npm run build:ru && npm run build:30s && firebase deploy --only hosting:codelab-next",
    "serve:ru": "npm run ng -- serve --configuration ru",
    "pre-deploy": "npm run build:prod && npm run build:ru && npm run build:30s",
    "deploy": "npm run pre-deploy && firebase deploy --only hosting:codelab",
    "cypress": "cypress",
    "cypress:approve": "npm run cypress run -- --env updateSnapshots=true",
    "cypress:open": "cypress open",
    "cypress:reporter": "npm run cypress run --reporter cypress-image-snapshot/reporter",
    "//--------------- KIRJS   ---------------------": "",
    "deploy:kirjs": "ng build kirjs --configuration production && firebase deploy  --only hosting:kirjs",
    "deploy:kirjs:ru": "ng build kirjs --configuration ru --configuration production && firebase deploy  --only hosting:kirjs",
    "serve:kirjs:ru": "ng serve kirjs --configuration ru",
    "build:kirjs:ru": "ng build kirjs --configuration ru",
    "build:kirjs": "ng build kirjs  --configuration production",
    "xi18n:kirjs": "ng xi18n kirjs --i18n-format xmb --output-path src/locale",
    "//--------------- Playground   ---------------------": "",
    "deploy:playground": "ng build playground --configuration production && firebase deploy --only hosting:angular-ivy",
    "serve:playground": "ng serve playground",
    "build:playground": "ng build playground  --configuration production",
    "//--------------- LIS   ---------------------": "",
    "deploy:lis": "ng build lis --configuration production && firebase deploy --only hosting:lis",
    "serve:lis": "ng serve lis",
    "build:lis": "ng build lis  --configuration production",
    "//--------------- 30s OF ANGULAR   -------------------": "",
    "build:30s": "npm run ng build angular-thirty-seconds --configuration production",
    "serve:30s": "ng serve angular-thirty-seconds",
    "//--------------- NX      ---------------------": "",
    "affected:apps": "./node_modules/.bin/nx affected:apps",
    "affected:libs": "./node_modules/.bin/nx affected:libs",
    "affected:build": "./node_modules/.bin/nx affected:build",
    "affected:test": "./node_modules/.bin/nx affected:test --watch=false",
    "affected:lint": "./node_modules/.bin/nx affected:lint",
    "affected:dep-graph": "./node_modules/.bin/nx affected:dep-graph",
    "format": "./node_modules/.bin/nx format:write",
    "format:write": "./node_modules/.bin/nx format:write",
    "format:check": "./node_modules/.bin/nx format:check",
    "update": "ng update @nrwl/workspace",
    "update:check": "ng update",
    "dep-graph": "./node_modules/.bin/nx dep-graph",
    "workspace-schematic": "./node_modules/.bin/nx workspace-schematic",
    "help": "./node_modules/.bin/nx help",
    "postinstall": "ngcc --properties es2015 browser module main"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format:write && git add ."
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:codelab-fun/codelab.git"
  },
  "public": true,
  "dependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-devkit/schematics": "14.2.6",
    "@angular/animations": "14.2.7",
    "@angular/cdk": "12.2.13",
    "@angular/common": "14.2.7",
    "@angular/compiler": "14.2.7",
    "@angular/core": "14.2.7",
    "@angular/fire": "^7.4.1",
    "@angular/forms": "14.2.7",
    "@angular/localize": "14.2.7",
    "@angular/material": "12.2.13",
    "@angular/platform-browser": "14.2.7",
    "@angular/platform-browser-dynamic": "14.2.7",
    "@angular/router": "14.2.7",
    "@babel/core": "^7.17.9",
    "@babel/generator": "^7.17.9",
    "@babel/traverse": "^7.17.9",
    "@ngxd/core": "^10.0.0",
    "angular2-medium-editor": "^1.3.1",
    "binary-loader": "0.0.1",
    "binary-parser": "^1.9.2",
    "chai": "^4.3.6",
    "console": "^0.7.2",
    "core-js": "2.6.2",
    "dom-to-image": "^2.6.0",
    "firebase": "^9.12.1",
    "flamelink": "^1.0.0-rc.1",
    "fullscreen-api-polyfill": "^1.1.2",
    "glob": "^7.2.0",
    "gray-matter": "^4.0.3",
    "husky": "^4.3.8",
    "immer": "^5.3.6",
    "inquirer": "^8.2.4",
    "medium-editor": "^5.23.3",
    "mocha": "^7.2.0",
    "monaco-editor": "^0.34.1",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "nanoid": "^3.3.3",
    "ng2-medium-editor": "^1.0.0",
    "ngx-file-drop": "^13.0.0",
    "ngx-markdown": "^8.2.2",
    "rangy": "^1.3.0",
    "raw-loader": "1.0.0",
    "rxjs": "^6.6.7",
    "slugify": "^1.6.5",
    "wabt": "^1.0.28",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.6",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular/cli": "14.2.6",
    "@angular/compiler-cli": "14.2.7",
    "@angular/language-service": "14.2.7",
    "@bahmutov/add-typescript-to-cypress": "^2.0.0",
    "@nrwl/angular": "14.0.3",
    "@nrwl/cypress": "14.0.3",
    "@nrwl/jest": "14.0.3",
    "@nrwl/workspace": "14.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "^3.6.11",
    "@types/jest": "25.1.2",
    "@types/node": "^13.13.52",
    "@types/node-fetch": "^2.6.1",
    "@typescript-eslint/eslint-plugin": "5.3.0",
    "@typescript-eslint/parser": "5.3.0",
    "babel-types": "^6.25.0",
    "babylon": "^6.17.3",
    "codelyzer": "^6.0.2",
    "cypress": "~4.0.1",
    "cypress-image-snapshot": "^3.0.0",
    "dotenv": "8.2.0",
    "eslint": "^8.2.0",
    "firebase-tools": "^11.15.0",
    "fs-extra": "^9.1.0",
    "github-api": "^3.4.0",
    "gomoku-tools": "^0.1.0",
    "googleapis": "^51.0.0",
    "jasmine-core": "~3.6.0",
    "jest": "25.1.0",
    "jest-preset-angular": "8.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "^3.1.1",
    "karma-cli": "~2.0.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^14.2.2",
    "node-fetch": "^2.6.7",
    "prettier": "2.6.2",
    "protractor": "~7.0.0",
    "systemjs": "0.20.12",
    "systemjs-builder": "^0.16.4",
    "ts-jest": "25.2.0",
    "ts-node": "^8.10.2",
    "tsickle": "^0.39.1",
    "tslib": "^2.4.0",
    "tslint": "~6.1.0",
    "typescript": "^4.8.4",
    "uglify-js": "^3.15.4",
    "webpack-dev-server": "^4.8.1"
  }
}
